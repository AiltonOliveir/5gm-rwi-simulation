<routes>
    <vTypeDistribution id="typeVehicleDistribution">
        <!--http://www.toyota.com.au/prius-v/specifications/prius-v-->
        <vType id="Car" departSpeed="max" accel="2.6" decel="4.5" length="4.645" width="1.775" height="1.59" maxSpeed="30.0" speedDev="0.1" sigma="0.2" minGap="0.3" probability="0.6"/>
        <!--http://www.rms.nsw.gov.au/business-industry/heavy-vehicles/road-access/general-access-vehicles.html-->
        <vType id="Truck" accel="2.0" decel="4" length="12.5" width="2.5" height="4.3" maxSpeed="25.0" speedDev="0.1" sigma="0.2" minGap="0.3" probability="0.2"/>
        <!--http://www.modenabus.com/286/119/products/school_bus_school_bus_iveco_9_metres.html-->
        <vType id="Bus" accel="2.0" decel="4" length="9" width="2.4" height="3.2" maxSpeed="20.0" speedDev="0.1" sigma="0.2" minGap="0.3" probability="0.2"/>
    </vTypeDistribution>
    <flow id="laneA" color="1,0,0" begin="0" end= "3000" probability="0.9" type="typeVehicleDistribution">
        <route edges="laneA"/>
    </flow>    
    <flow id="laneB" color="0,1,0"  begin="0" end= "3000" probability="0.95" type="typeVehicleDistribution">
        <route edges="laneB"/>
    </flow>    
</routes>